package com.sp.portal.brands;

import java.util.ArrayList;

import com.shine.boards.CorpBrand;

public class BrandResult {
	private CorpBrand brand;
	private ArrayList<String> errorMessages;
	
	public BrandResult(CorpBrand brand, ArrayList<String> errorMessages) {
		super();
		this.brand = brand;
		this.errorMessages = errorMessages;
	}
	
	public CorpBrand getBrand() {
		return brand;
	}
	public void setBrand(CorpBrand brand) {
		this.brand = brand;
	}
	public ArrayList<String> getErrorMessages() {
		return errorMessages;
	}
	public void setErrorMessages(ArrayList<String> errorMessages) {
		this.errorMessages = errorMessages;
	}
	
	

}
