package com.shine.objects;

public class Info extends ShineObject {

	private int nextUpdate;

	public int getNextUpdate() {
		return nextUpdate;
	}

	public void setNextUpdate(int nextUpdate) {
		this.nextUpdate = nextUpdate;
	}

	@Override
	public String toXml() {

		return "<info nextupdate=\"" + nextUpdate + "\"/>";
	}

}
